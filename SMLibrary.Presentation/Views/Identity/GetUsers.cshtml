@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityUser>

<div class="container">
    <div class="row">
        @foreach(var user in Model)
        {
            <div class="col-md-4 mb-4">
                <div class="card">
                    <div class="card-body" >
                        <h5 class="card-title">Name: @user.UserName</h5>
                        <p class="card-text">Email: @user.Email</p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

